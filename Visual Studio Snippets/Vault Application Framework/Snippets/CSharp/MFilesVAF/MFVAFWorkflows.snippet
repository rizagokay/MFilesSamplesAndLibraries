  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>State Action Code Snippet</Title>
      <Author>M-Files Corporation</Author>
      <Description>Adds a VAF handler for a state action.</Description>
      <HelpUrl>
      </HelpUrl>
      <Shortcut>MFVAFStateAction</Shortcut>
    </Header>
    <Snippet>
      <References>
        <Reference>
          
        </Reference>
      </References>
      <Imports>
        <Import>
          <Namespace>MFiles.VAF.Common</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal Editable="true">
          <ID>Alias</ID>
          <ToolTip>Replace with the workflow state alias.</ToolTip>
          <Default>MyWorkflowStateAlias</Default>
          <Function>
          </Function>
        </Literal>
        <Literal Editable="true">
          <ID>MethodName</ID>
          <ToolTip>The name of the method in C#.</ToolTip>
          <Default>MyWorkflowStateHandler</Default>
          <Function>
          </Function>
        </Literal>
      </Declarations>
      <Code Language="csharp" Kind="method decl" Delimiter="$"><![CDATA[
/// <summary>
/// Executed when an object is moved into a workflow state
/// with alias "$Alias$".
/// </summary>
/// <param name="env">The vault/object environment.</param>
[StateAction("$Alias$")]
public void $MethodName$(StateEnvironment env)
{
	$end$
	throw new System.NotImplementedException();
}
				]]></Code>
    </Snippet>
  </CodeSnippet>

      <Code Language="csharp" Kind="method decl" Delimiter="$"><![CDATA[
/// <summary>
/// Executed to test whether an object can move into the
/// workflow state with alias "$Alias$".
/// </summary>
/// <param name="env">The vault/object environment.</param>
/// <param name="message">The message, shown to the user if object cannot move into the state (method returns false).</param>
/// <returns>True if the object can move into the state, false otherwise.</returns>
[StatePreConditions("$Alias$")]
public bool $MethodName$(StateEnvironment env, out string message)
{
	$end$
	throw new System.NotImplementedException();
}
				]]></Code>
